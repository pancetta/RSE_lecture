  "ignorePaths": [
    "node_modules/**",
    ".git/**",
    "_build/**",
    ".jupyter_cache/**",
    "*.ipynb",
    "*.pyc",
    "**/__pycache__/**",
    "*.png",
    "*.jpg",
    "*.jpeg",
    "*.gif",
    "*.svg",
    "*.pdf",
    ".venv/**",
    "venv/**",
    "env/**",
    "exports/**",
    "*.lock"
  ],
  "ignoreRegExpList": [
    "/```[\\s\\S]*?```/g",
    "/`[^`]+`/g",
    "/https?:\\/\\/[^\\s]+/g",
    "/\\b[A-Z]{2,}\\b/g"
  ],
  "flagWords": [],
  "patterns": [
    {
      "name": "markdown-code-blocks",
      "pattern": "/```[\\s\\S]*?```/g"
    },
    {
      "name": "inline-code",
      "pattern": "/`[^`]+`/g"
    }
  ],
  "overrides": [
    {
      "filename": "**/*.py",
      "ignoreRegExpList": [
        "/```[\\s\\S]*?```/g",
        "/# ---$/gm",
        "/# %%.*$/gm",
        "/#!/gm",
        "/'[^']*'/g",
        "/\"[^\"]*\"/g"
      ]
    },
    {
      "filename": "**/*.md",
      "ignoreRegExpList": [
        "/```[\\s\\S]*?```/g",
        "/`[^`]+`/g",
        "/https?:\\/\\/[^\\s]+/g"
      ]
    }
  ]
